{
  "kind": "collectionType",
  "collectionName": "clientes",
  "info": {
    "name": "Clientes",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string",
      "required": true
    },
    "cnpj": {
      "type": "string",
      "required": true,
      "regex": "\\d"
    },
    "endereco": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "senha": {
      "type": "password",
      "required": true
    },
    "logo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "telefone": {
      "type": "string",
      "regex": "\\d"
    }
  }
}
